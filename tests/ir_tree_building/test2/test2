class Main {
    public static void main() {
        A a;
        int b;
        b = a.square(b) + a.double(b) + a.inc(b);
    }
}

class A {
    public int square(int a) {
        return a * a;
    }

    public int double(int a) {
        return 2 * a;
    }

    public int inc(int a) {
        return a + 1;
    }
}
