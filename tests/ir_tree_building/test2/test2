class Main {
    public static void main() {
        int a;
        a = 2 + 2 * 2 + (2 + 2) * 2;
        bool b;
        b = true;

        if (a > 5 && b)
            if (a > 3)
                System.out.println(a + a);
            else
                System.out.println(a + 1);
        
        assert(a > 0);
    }
}
